  "devDependencies": {
    "@eslint/js": "^9.25.0",
    "@types/plotly.js": "^3.0.0",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@types/react-plotly.js": "^2.6.3",
    "@vitejs/plugin-react": "^4.4.1",
    "eslint": "^9.25.0",
